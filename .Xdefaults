! color depth
URxvt*depth: 32

! fonts
URxvt.font:       xft:Inconsolata:pixelsize=12
URxvt.boldFont:   xft:Inconsolata:bold:pixelsize=12
urxvtq.font:      xft:Inconsolata:pixelsize=12
urxvtq.boldFont:  xft:Inconsolata:bold:pixelsize=12

! scrollbar
URxvt*scrollBar: False
URxvt*secondaryScroll: False

! bell
URxvt*urgentOnBell: true

! save lines
URxvt*saveLines: 4096

! border
URxvt*internalBorder: 0
URxvt*externalBorder: 0

! url launcher
URxvt*url-launcher: /usr/bin/google-chrome-beta
URxvt.matcher.button: 1 

! colors
! main colors
URxvt*background: #081828
URxvt*foreground: #DEDEDE
URxvt*colorUL: #86a2b0
! black
URxvt*color0  : #2E3436
URxvt*color8  : #081828
! red
URxvt*color1  : #CC0000
URxvt*color9  : #EF2929
! green
URxvt*color2  : #4E9A06
URxvt*color10 : #8AE234
! yellow
URxvt*color3  : #C4A000
URxvt*color11 : #FCE94F
! blue
URxvt*color4  : #3465A4
URxvt*color12 : #729FCF
! magenta
URxvt*color5  : #75507B
URxvt*color13 : #AD7FA8
! cyan
URxvt*color6  : #06989A
URxvt*color14 : #34E2E2
! white
URxvt*color7  : #D3D7CF
URxvt*color15 : #EEEEEC

! extensions
URxvt.perl-ext-common: default,tabbedex,matcher,clipboard

! tabbar
URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 2
URxvt.tabbed.tab-bg: 8
URxvt.tabbed.autohide:  false
URxvt.tabbed.new-button:  false
URxvt.tabbed.title: yes

! disable iso14755
URxvt.iso14755: False

! clipboard
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
